{
  "description": "",
  "name": "fykosak/fksdb",
  "require": {
    "php": ">=7.4.0",
    "nette/utils": "^3.2.2",
    "nette/security": "^3.1.3",
    "nette/finder": "^2.5.2",
    "nette/http": "^3.1.1",
    "nette/robot-loader": "^3.4.0",
    "nette/safe-stream": "^2.4.1",
    "nette/component-model": "^3.0.1",
    "nette/mail": "^3.1.6",
    "nette/neon": "^3.2.2",
    "nette/forms": "^3.1.3",
    "nette/php-generator": "^3.5.1",
    "nette/database": "^3.1.1",
    "nette/caching": "^3.1.1",
    "latte/latte": "^2.10.3",
    "tracy/tracy": "^2.8.1",
    "nette/tester": "^2.4.0",
    "nette/application": "^3.1.2",
    "nette/di": "^3.0.8",
    "nette/bootstrap": "^3.1.1",
    "nette/tokenizer": "^3.1.0",
    "nette/nette": "^3.1.0",
    "setasign/fpdf": "1.8.2",
    "setasign/fpdi": "1.5.4",
    "league/oauth2-google": "3.0.3",
    "fykosak/nette-orm": "dev-dev-0.2.3"
  },
  "autoload": {
    "psr-4": {
      "FKSDB\\": "app/",
      "FKSDB\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "initTestDatabase": [
      "./tests/init-database.sh"
    ],
    "clearTestDatabase": [
      "./tests/clear-database.sh"
    ],
    "clearTestCache": [
      "rm -rf temp/tester/"
    ],
    "clearCache": [
      "rm -rf temp/cache/"
    ],
    "test": [
      "@clearTestCache",
      "@clearTestDatabase",
      "vendor/bin/tester tests -p php -s -c tests/php.ini -j 8"
    ],
    "testCoverage": [
      "@clearTestCache",
      "@clearTestDatabase",
      "vendor/bin/tester tests -p php --coverage coverage.html --coverage-src app -s -c tests/php.ini -j 8"
    ]
  }
}
