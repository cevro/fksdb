{layout '../@layout.latte'}
{block content}
    {dump $models}
    {if count($models)}
        {foreach $models as $model}
            <h3 class="card-header">{control 'valuePrinter__'.$table.'__'.$itemHeadline:onlyValue $model}</h3>
            <ul class="list-group list-group-flush">
                {foreach $rows as $field}
                    {control 'valuePrinter__'.$table.'__'.$field:listItem $model}
                {/foreach}
            </ul>
        {/foreach}
    {else}
        <div class="card-body">
            {control noRecords}
        </div>
    {/if}
{/block}
